<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:aapt="http://schemas.android.com/aapt" xmlns:app="http://schemas.android.com/apk/res-auto">
    <item>
        <shape android:shape="oval">
            <padding android:bottom="15dp" android:left="15dp" android:right="15dp" android:top="15dp" />
            <stroke android:width="1.5dp" android:color="#000000" android:dashWidth="0dp" android:dashGap="0dp" />
            <gradient android:angle="90" android:endColor="#fcfcfc" android:startColor="#ffffff" />
            <size android:width="15dp" android:height="15dp" />
        </shape>
    </item>
</selector>
